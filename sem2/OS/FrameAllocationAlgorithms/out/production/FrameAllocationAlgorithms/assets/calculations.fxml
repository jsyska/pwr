<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="487.0" prefWidth="633.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Calculations">
    <children>
        <BarChart fx:id="pagesChart" layoutX="-1.0" layoutY="81.0" legendVisible="false" prefHeight="310.0" prefWidth="634.0" title="RESULTS">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </BarChart>
        <Button fx:id="back" layoutX="287.0" layoutY="55.0" mnemonicParsing="false" onAction="#goBack" text="Go back" />
        <Label fx:id="faults" layoutX="273.0" layoutY="397.0" text="Label" />
      <Button fx:id="detailedResults" layoutX="253.0" layoutY="14.0" mnemonicParsing="false" onAction="#showDetailedResults" text="Show detailed results" />
    </children>
</AnchorPane>
